<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="assets_backend" name="wages_assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <!--饼图所需导包-->
                <script src="/itsm_reception/static/src/js/echarts.js"></script>
                <script src="/itsm_reception/static/src/js/higtcharts.js"></script>
                <script src="/itsm_reception/static/src/js/highcharts-more.js"></script>
                <script type="text/javascript" src="/itsm_reception/static/src/js/xml/highCharts_zhuti.js"></script>
                <!--VUE导包-->
                <script src="https://cdn.jsdelivr.net/npm/vue@2.6.9/dist/vue.js"></script>
                <!-- Element -->
                <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css"/>
                <script src="https://unpkg.com/element-ui/lib/index.js"></script>
                <script type="text/javascript" src="/itsm_reception/static/src/js/star.js"></script>
                <script type="text/javascript" src="/itsm_reception/static/src/js/evaluate.js"></script>
                <script type="text/javascript" src="/itsm_reception/static/src/js/bing.js"></script>
                <script type="text/javascript" src="/itsm_reception/static/src/js/zhu.js"></script>
